{
  "manifest_version": 2,
  "name": "AIFEX | AI For Exploratory testing",
  "version": "0.11.0",
  "description": "Help tester to collaborate on exploratory testing sessions",
  "permissions": [
    "<all_urls>",
    "webNavigation",
    "tabs"
  ],
  "icons": {
    "128": "images/aifex_icon.png"
  },
  "browser_action": {
    "default_title": "AIFEX",
    "default_popup": "aifex_page/index.html"
  },
  "background": {
    "scripts": ["index.js"]
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "extension.aifex@gmail.com"
    }
  }
}
